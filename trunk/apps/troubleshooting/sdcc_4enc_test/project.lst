0035e6   6ef6     movwf	0xf6, 0            	movwf	TBLPTRL
0035e8   cfe9     movff	0xfe9, 0xfd9       	movff	FSR0L, FSR2L
0035ea   ffd9
0035ec   cfdc     movff	0xfdc, 0xff7       	movff	PREINC2, TBLPTRH
0035ee   fff7
0035f0   50dc     movf	0xdc, 0, 0          	movf	PREINC2, W
                                           
                                           	;; SDCC passes a 24bit pointer, the MSB (now in WREG) bit 6 and 7 tells us, where the data is located:
                                           	;; 00 -> code
                                           	;; 01 -> EEPROM (not supported by MIOS_LCD_PrintCString yet)
                                           	;; 10 -> data
                                           	;; 11 -> unimplemented
                                           
0035f2   bee8     btfsc	0xe8, 0x7, 0       	btfsc	WREG, 7
0035f4   d008     bra	0x3606               	bra	_MIOS_LCD_PrintCString_RAM
                                           _MIOS_LCD_PrintCString_Code
0035f6   6ef8     movwf	0xf8, 0            	movwf	TBLPTRU
                                           _MIOS_LCD_PrintCStringLoop_Code
0035f8   0009     tblrd	*+                 	tblrd*+
0035fa   50f5     movf	0xf5, 0, 0          	movf	TABLAT, W
0035fc   e003     bz	0x3604                	bz	_MIOS_LCD_PrintCString_Code_End
0035fe   ec94     call	0x2d28, 0           	call	MIOS_LCD_PrintChar
003600   f016
003602   d7fa     bra	0x35f8               	bra	_MIOS_LCD_PrintCStringLoop_Code
                                           _MIOS_LCD_PrintCString_Code_End
003604   0012     return	0                 	return
                                           
                                           _MIOS_LCD_PrintCString_RAM
                                           	;; using FSR2 as temporary pointer - store current FSR2 into TMP[12]
003606   cfd9     movff	0xfd9, 0x6         	movff	FSR2L, TMP1
003608   f006
00360a   cfda     movff	0xfda, 0x7         	movff	FSR2H, TMP2
00360c   f007
00360e   cff6     movff	0xff6, 0xfd9       	movff	TBLPTRL, FSR2L
003610   ffd9
003612   cff7     movff	0xff7, 0xfda       	movff	TBLPTRH, FSR2H
003614   ffda
                                           _MIOS_LCD_PrintCStringLoop_RAM
003616   50de     movf	0xde, 0, 0          	movf	POSTINC2, W
003618   e003     bz	0x3620                	bz	_MIOS_LCD_PrintCString_RAM_End
00361a   ec94     call	0x2d28, 0           	call	MIOS_LCD_PrintChar
00361c   f016
00361e   d7fb     bra	0x3616               	bra	_MIOS_LCD_PrintCStringLoop_RAM
                                           _MIOS_LCD_PrintCString_RAM_End
                                           	;; restore FSR2
003620   c006     movff	0x6, 0xfd9         	movff	TMP1, FSR2L
003622   ffd9
003624   c007     movff	0x7, 0xfda         	movff	TMP2, FSR2H
003626   ffda
