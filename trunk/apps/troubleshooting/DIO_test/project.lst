                                           .MIOS_LCD_PrintCString code
                                           _MIOS_LCD_PrintCString
                                           	global	_MIOS_LCD_PrintCString
                                           
0036be   6ef6     movwf	0xf6, 0            	movwf	TBLPTRL
0036c0   cfe9     movff	0xfe9, 0xfd9       	movff	FSR0L, FSR2L
0036c2   ffd9
0036c4   cfdc     movff	0xfdc, 0xff7       	movff	PREINC2, TBLPTRH
0036c6   fff7
0036c8   50dc     movf	0xdc, 0, 0          	movf	PREINC2, W
                                           
                                           	;; SDCC passes a 24bit pointer, the MSB (now in WREG) bit 6 and 7 tells us, where the data is located:
                                           	;; 00 -> code
                                           	;; 01 -> EEPROM (not supported by MIOS_LCD_PrintCString yet)
                                           	;; 10 -> data
                                           	;; 11 -> unimplemented
                                           
0036ca   bee8     btfsc	0xe8, 0x7, 0       	btfsc	WREG, 7
0036cc   d008     bra	0x36de               	bra	_MIOS_LCD_PrintCString_RAM
                                           _MIOS_LCD_PrintCString_Code
0036ce   6ef8     movwf	0xf8, 0            	movwf	TBLPTRU
                                           _MIOS_LCD_PrintCStringLoop_Code
0036d0   0009     tblrd	*+                 	tblrd*+
0036d2   50f5     movf	0xf5, 0, 0          	movf	TABLAT, W
0036d4   e003     bz	0x36dc                	bz	_MIOS_LCD_PrintCString_Code_End
0036d6   ec94     call	0x2d28, 0           	call	MIOS_LCD_PrintChar
0036d8   f016
0036da   d7fa     bra	0x36d0               	bra	_MIOS_LCD_PrintCStringLoop_Code
                                           _MIOS_LCD_PrintCString_Code_End
0036dc   0012     return	0                 	return
                                           
                                           _MIOS_LCD_PrintCString_RAM
                                           	;; using FSR2 as temporary pointer - store current FSR2 into TMP[12]
0036de   cfd9     movff	0xfd9, 0x6         	movff	FSR2L, TMP1
0036e0   f006
0036e2   cfda     movff	0xfda, 0x7         	movff	FSR2H, TMP2
0036e4   f007
0036e6   cff6     movff	0xff6, 0xfd9       	movff	TBLPTRL, FSR2L
0036e8   ffd9
0036ea   cff7     movff	0xff7, 0xfda       	movff	TBLPTRH, FSR2H
0036ec   ffda
                                           _MIOS_LCD_PrintCStringLoop_RAM
0036ee   50de     movf	0xde, 0, 0          	movf	POSTINC2, W
0036f0   e003     bz	0x36f8                	bz	_MIOS_LCD_PrintCString_RAM_End
0036f2   ec94     call	0x2d28, 0           	call	MIOS_LCD_PrintChar
0036f4   f016
0036f6   d7fb     bra	0x36ee               	bra	_MIOS_LCD_PrintCStringLoop_RAM
                                           _MIOS_LCD_PrintCString_RAM_End
                                           	;; restore FSR2
0036f8   c006     movff	0x6, 0xfd9         	movff	TMP1, FSR2L
0036fa   ffd9
0036fc   c007     movff	0x7, 0xfda         	movff	TMP2, FSR2H
0036fe   ffda
003700   0012     return	0                 	return
                                           
