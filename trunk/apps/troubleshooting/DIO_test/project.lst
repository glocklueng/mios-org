0034fa   f03d
0034fc   ec80     call	0x3300, 0           	call	_Timer			; call C hook
0034fe   f019
003500   c33f     movff	0x33f, 0xfe9       	movff	STACK_IRQ_HEAD-0, FSR0L	; restore FSR0[LH] directly from memory (more fail save)
003502   ffe9
003504   c33e     movff	0x33e, 0xfea       	movff	STACK_IRQ_HEAD-1, FSR0H	; restore FSR0[LH] directly from memory (more fail save)
003506   ffea
