                                           	extern	_Timer
                                           
0034ec   ee23     lfsr	0x2, 0x33f          	lfsr	FSR2, STACK_IRQ_HEAD	; initialize IRQ stack
0034ee   f03f
0034f0   cfe9     movff	0xfe9, 0xfdd       	movff	FSR0L, POSTDEC2		; save FSR0[LH] on IRQ stack
0034f2   ffdd
0034f4   cfea     movff	0xfea, 0xfdd       	movff	FSR0H, POSTDEC2
0034f6   ffdd
